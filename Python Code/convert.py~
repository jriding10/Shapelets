#! /usr/bin/env python

## Takes a column vector and creates an image

def convert( data )
import matplotlib.pyplot as plt
import math as m

pxls = int(sqrt(data.shape[0]))

k=-1
for i in range (0, pxls)
	for j in range (0,pxls)
		k=k+1	
		model[i,j] = data[k];
	end
end

plt.imshow(model)
plt.show() 

